{
  "build": {
    "content": [
      {
        "files": [
          "docs/**.md",
          "docs/**/toc.yml",
          "*.md",
          "toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "favicon/**",
          "*.schema.json"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "Xappium UITest",
      "_appFooter": "Copyright 2022 Xappium",
      "_appLogoPath": "images/xappium-bot.png"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/material"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": true
  }
}
